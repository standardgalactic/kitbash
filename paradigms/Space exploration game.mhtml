</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">laplace</span></span><span>(</span><span><span class="hljs-params">U</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> (
        np.roll(U, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>) + np.roll(U, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>) +
        np.roll(U, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>) + np.roll(U, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>) - </span><span><span class="hljs-number">4</span></span><span>*U
    ) / (h*h)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">grad</span></span><span>(</span><span><span class="hljs-params">U</span></span><span>):
    gx = (np.roll(U, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>) - np.roll(U, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)) / (</span><span><span class="hljs-number">2</span></span><span>*h)  </span><span><span class="hljs-comment"># x ~ cols</span></span><span>
    gy = (np.roll(U, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>) - np.roll(U, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>)) / (</span><span><span class="hljs-number">2</span></span><span>*h)  </span><span><span class="hljs-comment"># y ~ rows</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> gx, gy

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">div</span></span><span>(</span><span><span class="hljs-params">vx, vy</span></span><span>):
    dvx_dx = (np.roll(vx, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>) - np.roll(vx, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)) / (</span><span><span class="hljs-number">2</span></span><span>*h)
    dvy_dy = (np.roll(vy, -</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>) - np.roll(vy, </span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>)) / (</span><span><span class="hljs-number">2</span></span><span>*h)
    </span><span><span class="hljs-keyword">return</span></span><span> dvx_dx + dvy_dy
</span></span></code></div></div></pre>
