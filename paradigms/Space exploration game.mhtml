function ethicsTensor() { const [gvx_x, gvx_y] = grad(vx); const [gvy_x, gvy_y] = grad(vy); const [gPx, gPy] = grad(Phi); const E = new Float32Array(W*H); for (let i=0;i&lt;W*H;i++) E[i] = gvx_x[i]*gPx[i] + gvy_y[i]*gPy[i]; return E; }
