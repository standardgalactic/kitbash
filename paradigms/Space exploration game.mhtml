    Phi, S, vx, vy = state.Phi, state.S, state.vx, state.vy
    </span><span><span class="hljs-comment"># 1) finite differences</span></span><span>
    lap_Phi = laplace(Phi); lap_S = laplace(S)
    gradPhi_x, gradPhi_y = grad(Phi); gradS_x, gradS_y = grad(S)
    div_v = div(vx, vy)
    lap_vx, lap_vy = laplace(vx), laplace(vy)
    </span><span><span class="hljs-comment"># curlcurl(v) = grad(div v) - lap v</span></span><span>
    gdiv_x, gdiv_y = grad(div_v)
    cc_x, cc_y = gdiv_x - lap_vx, gdiv_y - lap_vy

    </span><span><span class="hljs-comment"># 2) PDE updates (explicit Euler)</span></span><span>
    Phi += dt * (params.kPhi * lap_Phi - params.lmbd * S)
    S   += dt * (params.kS   * lap_S + params.gamma * (gradPhi_x**</span><span><span class="hljs-number">2</span></span><span> + gradPhi_y**</span><span><span class="hljs-number">2</span></span><span>) - params.muS * S)
    vx  += dt * (params.kv * cc_x - gradS_x - params.muv * vx)
    vy  += dt * (params.kv * cc_y - gradS_y - params.muv * vy)

    </span><span><span class="hljs-comment"># 3) Buildings/tech modifiers</span></span><span>
    apply_building_operators(state, params)

    </span><span><span class="hljs-comment"># 4) Pop/economy (optional)</span></span><span>
    update_population_and_resources(state, params)

    </span><span><span class="hljs-comment"># 5) Clamp &amp; sanitize</span></span><span>
    Phi = np.clip(Phi, </span><span><span class="hljs-number">0</span></span><span>, params.Phi_max)
    S   = np.clip(S,   </span><span><span class="hljs-number">0</span></span><span>, params.S_max)
    state.Phi, state.S, state.vx, state.vy = Phi, S, vx, vy
