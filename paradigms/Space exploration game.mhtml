</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'btn-prev'</span></span><span>).</span><span><span class="hljs-property">onclick</span></span><span> = </span><span><span class="hljs-function">()=&gt;</span></span><span>{ turn=</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">max</span></span><span>(turn-</span><span><span class="hljs-number">1</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>); </span><span><span class="hljs-title function_">draw</span></span><span>(states[turn]); };

</span><span><span class="hljs-title function_">loadStates</span></span><span>();
