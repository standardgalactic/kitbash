    dphi = -</span><span><span class="hljs-number">0.3</span></span><span> * grad_phi + </span><span><span class="hljs-number">0.1</span></span><span> * (tile[</span><span><span class="hljs-string">'S'</span></span><span>] - np.mean([n[</span><span><span class="hljs-string">'S'</span></span><span>] </span><span><span class="hljs-keyword">for</span></span><span> n </span><span><span class="hljs-keyword">in</span></span><span> neighbors]))
    dS = </span><span><span class="hljs-number">0.2</span></span><span> * grad_phi**</span><span><span class="hljs-number">2</span></span><span> - </span><span><span class="hljs-number">0.05</span></span><span> * tile[</span><span><span class="hljs-string">'S'</span></span><span>]

    tile[</span><span><span class="hljs-string">'phi'</span></span><span>] += dphi
    tile[</span><span><span class="hljs-string">'S'</span></span><span>] += dS
    </span><span><span class="hljs-keyword">return</span></span><span> tile
