  let html = '&lt;table&gt;&lt;thead&gt;&lt;tr&gt;&lt;th&gt;Tech&lt;/th&gt;&lt;th&gt;Description&lt;/th&gt;&lt;th&gt;Prereq&lt;/th&gt;&lt;th&gt;Status&lt;/th&gt;&lt;/tr&gt;&lt;/thead&gt;&lt;tbody&gt;';
  for (const r of rows){
    const [name,desc,pre,unlocked] = r;
