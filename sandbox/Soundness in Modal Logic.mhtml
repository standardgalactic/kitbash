    </span><span><span class="hljs-keyword">return</span></span><span> gaussian_filter(Φ, sigma=</span><span><span class="hljs-number">1.0</span></span><span>) * (</span><span><span class="hljs-number">1</span></span><span> - entropy_weight) + Φ * entropy_weight

