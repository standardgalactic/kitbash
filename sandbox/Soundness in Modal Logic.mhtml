            j_offset = </span><span><span class="hljs-built_in">int</span></span><span>(np.clip(j + alpha * vy * grid_size, </span><span><span class="hljs-number">0</span></span><span>, grid_size - </span><span><span class="hljs-number">1</span></span><span>))
            dΦ[i, j] = Φ[i_offset, j_offset]
    </span><span><span class="hljs-keyword">return</span></span><span> dΦ

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">apply_entropy_flow</span></span><span>(</span><span><span class="hljs-params">Φ, entropy_weight=<span class="hljs-number">0.05</span></span></span><span>):
