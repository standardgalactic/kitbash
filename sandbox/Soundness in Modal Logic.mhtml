7 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np
</span><span><span class="hljs-keyword">import</span></span><span> matplotlib.pyplot </span><span><span class="hljs-keyword">as</span></span><span> plt
</span><span><span class="hljs-keyword">from</span></span><span> scipy.ndimage </span><span><span class="hljs-keyword">import</span></span><span> gaussian_filter

</span><span><span class="hljs-comment"># Grid size</span></span><span>
grid_size = </span><span><span class="hljs-number">64</span></span><span>

</span><span><span class="hljs-comment"># Initialize fields</span></span><span>
Φ = np.zeros((grid_size, grid_size))
𝒗 = np.zeros((grid_size, grid_size, </span><span><span class="hljs-number">2</span></span><span>))  </span><span><span class="hljs-comment"># vector field</span></span><span>
𝒮 = np.zeros((grid_size, grid_size))     </span><span><span class="hljs-comment"># entropy field</span></span><span>

</span><span><span class="hljs-comment"># Create a radial attractor (Löb-stable candidate)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_lob_stable_field</span></span><span>():
    x = np.linspace(-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, grid_size)
    y = np.linspace(-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, grid_size)
    X, Y = np.meshgrid(x, y)
    radius = np.sqrt(X**</span><span><span class="hljs-number">2</span></span><span> + Y**</span><span><span class="hljs-number">2</span></span><span>)
    Φ = np.exp(-</span><span><span class="hljs-number">10</span></span><span> * radius**</span><span><span class="hljs-number">2</span></span><span>)  </span><span><span class="hljs-comment"># Gaussian attractor</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> Φ

</span><span><span class="hljs-comment"># Create a rotating spiral field (Gödel-incomplete candidate)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_godel_field</span></span><span>():
    x = np.linspace(-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, grid_size)
    y = np.linspace(-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>, grid_size)
    X, Y = np.meshgrid(x, y)
    angle = np.arctan2(Y, X)
    radius = np.sqrt(X**</span><span><span class="hljs-number">2</span></span><span> + Y**</span><span><span class="hljs-number">2</span></span><span>)
    Φ = np.sin(</span><span><span class="hljs-number">10</span></span><span> * angle + </span><span><span class="hljs-number">20</span></span><span> * radius)
    </span><span><span class="hljs-keyword">return</span></span><span> Φ

</span><span><span class="hljs-comment"># Apply recursive vector smoothing (simplified box operator)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">recursive_smooth</span></span><span>(</span><span><span class="hljs-params">Φ, steps=<span class="hljs-number">10</span></span></span><span>, sigma=</span><span><span class="hljs-number">1.0</span></span><span>):
    </span><span><span class="hljs-keyword">for</span></span><span> _ </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(steps):
        Φ = gaussian_filter(Φ, sigma=sigma)
    </span><span><span class="hljs-keyword">return</span></span><span> Φ

