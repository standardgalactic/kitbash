</span><span><span class="hljs-keyword">\For</span></span><span>{</span><span><span class="hljs-built_in">$</span></span><span>t=W,</span><span><span class="hljs-keyword">\dots</span></span><span><span class="hljs-built_in">$</span></span><span>}
