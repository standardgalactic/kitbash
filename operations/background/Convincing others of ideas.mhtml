</span><span><span class="hljs-keyword">\hat</span></span><span> D</span><span><span class="hljs-built_in">^</span></span><span><span class="hljs-keyword">\ast</span></span><span> </span><span><span class="hljs-keyword">\;</span></span><span>=</span><span><span class="hljs-keyword">\;</span></span><span> </span><span><span class="hljs-keyword">\arg</span></span><span><span class="hljs-keyword">\max</span></span><span><span class="hljs-built_in">_</span></span><span>{D</span><span><span class="hljs-keyword">\in</span></span><span><span class="hljs-keyword">\{</span></span><span>1,</span><span><span class="hljs-keyword">\dots</span></span><span>,D</span><span><span class="hljs-built_in">_</span></span><span>{</span><span><span class="hljs-keyword">\max</span></span><span>}</span><span><span class="hljs-keyword">\}</span></span><span>} </span><span><span class="hljs-keyword">\frac</span></span><span>{</span><span><span class="hljs-keyword">\prod</span></span><span><span class="hljs-built_in">_</span></span><span>{l=1}</span><span><span class="hljs-built_in">^</span></span><span>{D}</span><span><span class="hljs-keyword">\widehat</span></span><span><span class="hljs-keyword">\chi</span></span><span><span class="hljs-built_in">^</span></span><span>l}{</span><span><span class="hljs-keyword">\lambda</span></span><span> D}.
