  20% { mask-position: 0px center; }
  100% { mask-position: 0px center; }
}

@keyframes loading-results-shimmer { 
  0% { background-position: -1000px 0px; }
  100% { background-position: 1000px 0px; }
}

@keyframes scalePulse { 
  0%, 100% { filter: blur(); transform: scale(1); }
  50% { filter: blur(8px); transform: scale(1.1); }
}

.bg-scale-pulse { animation: 3.5s ease-in-out 0s infinite normal none running scalePulse; transform-origin: 50% center; }

@keyframes diagonalSweep { 
  0% { transform: translate(-100%, -100%); }
  100% { transform: translate(100%, 100%); }
}

.diagonal-sweep-gradient { animation: 4s ease-out 0s infinite normal none running diagonalSweep; }

[dir="ltr"] .diagonal-sweep-gradient { background-image: linear-gradient(135deg, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.2), rgba(255, 255, 255, 0) 54%); }

[dir="rtl"] .diagonal-sweep-gradient { background-image: linear-gradient(-135deg, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.2), rgba(255, 255, 255, 0) 54%); }

@media (prefers-reduced-motion: reduce) {
  .diagonal-sweep-gradient { animation: auto ease 0s 1 normal none running none; }
}

