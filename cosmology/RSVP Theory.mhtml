6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copiar código</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np
</span><span><span class="hljs-keyword">import</span></span><span> matplotlib.pyplot </span><span><span class="hljs-keyword">as</span></span><span> plt
</span><span><span class="hljs-keyword">from</span></span><span> matplotlib.animation </span><span><span class="hljs-keyword">import</span></span><span> FuncAnimation

</span><span><span class="hljs-comment"># Grid parameters</span></span><span>
nx, ny = </span><span><span class="hljs-number">100</span></span><span>, </span><span><span class="hljs-number">100</span></span><span>
dx, dy = </span><span><span class="hljs-number">1.0</span></span><span>, </span><span><span class="hljs-number">1.0</span></span><span>
dt = </span><span><span class="hljs-number">0.1</span></span><span>
steps = </span><span><span class="hljs-number">200</span></span><span>

</span><span><span class="hljs-comment"># Physical parameters</span></span><span>
D_m = </span><span><span class="hljs-number">0.2</span></span><span>     </span><span><span class="hljs-comment"># diffusivity of matter</span></span><span>
gamma = </span><span><span class="hljs-number">0.05</span></span><span>  </span><span><span class="hljs-comment"># diffusivity of Phi</span></span><span>
lambda_phi = </span><span><span class="hljs-number">0.01</span></span><span>  </span><span><span class="hljs-comment"># decay of Phi</span></span><span>
eta = </span><span><span class="hljs-number">0.5</span></span><span>     </span><span><span class="hljs-comment"># coupling to surface tension</span></span><span>
sigma_0 = </span><span><span class="hljs-number">1.0</span></span><span>
delta = </span><span><span class="hljs-number">1.0</span></span><span>
rho_c = </span><span><span class="hljs-number">1.0</span></span><span>
chi = </span><span><span class="hljs-number">1.0</span></span><span>
xi = </span><span><span class="hljs-number">0.5</span></span><span>

</span><span><span class="hljs-comment"># Initialize fields</span></span><span>
rho_m = np.zeros((nx, ny))
Phi = np.zeros((nx, ny))
sigma = np.zeros((nx, ny))

</span><span><span class="hljs-comment"># Initial condition: clump of matter at center</span></span><span>
x = np.arange(nx)
y = np.arange(ny)
X, Y = np.meshgrid(x, y)
rho_m += np.exp(-((X - nx/</span><span><span class="hljs-number">2</span></span><span>)**</span><span><span class="hljs-number">2</span></span><span> + (Y - ny/</span><span><span class="hljs-number">2</span></span><span>)**</span><span><span class="hljs-number">2</span></span><span>) / </span><span><span class="hljs-number">100</span></span><span>)

</span><span><span class="hljs-comment"># Initial Phi field (detergent-like inflationary field)</span></span><span>
Phi += </span><span><span class="hljs-number">0.2</span></span><span> * np.random.rand(nx, ny)

</span><span><span class="hljs-comment"># Function to compute Laplacian</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">laplacian</span></span><span>(</span><span><span class="hljs-params">Z</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> (np.roll(Z, </span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">0</span></span><span>) + np.roll(Z, -</span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">0</span></span><span>) +
            np.roll(Z, </span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>) + np.roll(Z, -</span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>) - </span><span><span class="hljs-number">4</span></span><span> * Z) / (dx * dy)

</span><span><span class="hljs-comment"># Prepare plot</span></span><span>
fig, ax = plt.subplots()
img = ax.imshow(rho_m, cmap=</span><span><span class="hljs-string">'plasma'</span></span><span>, interpolation=</span><span><span class="hljs-string">'bilinear'</span></span><span>, vmin=</span><span><span class="hljs-number">0</span></span><span>, vmax=</span><span><span class="hljs-number">1</span></span><span>)
plt.title(</span><span><span class="hljs-string">"Matter Density (ρₘ)"</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">update</span></span><span>(</span><span><span class="hljs-params">frame</span></span><span>):
    </span><span><span class="hljs-keyword">global</span></span><span> rho_m, Phi, sigma

    </span><span><span class="hljs-comment"># Update surface tension field</span></span><span>
    sigma = sigma_0 * np.exp(-delta * Phi) * (</span><span><span class="hljs-number">1</span></span><span> - rho_m / rho_c)

    </span><span><span class="hljs-comment"># Compute curvature-driven flow field (simplified as potential flow)</span></span><span>
    v_x = -chi * np.gradient(sigma, axis=</span><span><span class="hljs-number">0</span></span><span>) + xi * np.gradient(Phi, axis=</span><span><span class="hljs-number">0</span></span><span>)
    v_y = -chi * np.gradient(sigma, axis=</span><span><span class="hljs-number">1</span></span><span>) + xi * np.gradient(Phi, axis=</span><span><span class="hljs-number">1</span></span><span>)

    </span><span><span class="hljs-comment"># Update matter density</span></span><span>
    lap_rho = laplacian(rho_m)
    rho_m += dt * (D_m * lap_rho - np.gradient(sigma, axis=</span><span><span class="hljs-number">0</span></span><span>)[</span><span><span class="hljs-number">0</span></span><span>])

    </span><span><span class="hljs-comment"># Update Phi field</span></span><span>
    lap_Phi = laplacian(Phi)
    Phi += dt * (gamma * lap_Phi - lambda_phi * Phi + eta * sigma)

    </span><span><span class="hljs-comment"># Enforce bounds</span></span><span>
    rho_m = np.clip(rho_m, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)
    Phi = np.clip(Phi, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">2</span></span><span>)

    </span><span><span class="hljs-comment"># Update plot</span></span><span>
    img.set_data(rho_m)
    </span><span><span class="hljs-keyword">return</span></span><span> [img]

ani = FuncAnimation(fig, update, frames=steps, interval=</span><span><span class="hljs-number">50</span></span><span>, blit=</span><span><span class="hljs-literal">True</span></span><span>)
plt.close()
ani
