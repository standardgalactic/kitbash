</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">deduplicate_bib</span></span><span>(</span><span><span class="hljs-params">input_file</span></span><span>):
    entries = {}
    current_key = </span><span><span class="hljs-literal">None</span></span><span>
    current_entry = []
    inside_entry = </span><span><span class="hljs-literal">False</span></span><span>

    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(input_file, </span><span><span class="hljs-string">"r"</span></span><span>, encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        lines = f.readlines()

    </span><span><span class="hljs-keyword">for</span></span><span> line </span><span><span class="hljs-keyword">in</span></span><span> lines:
        </span><span><span class="hljs-keyword">if</span></span><span> line.strip().startswith(</span><span><span class="hljs-string">"@"</span></span><span>):
            </span><span><span class="hljs-comment"># Save the previous entry before starting a new one</span></span><span>
            </span><span><span class="hljs-keyword">if</span></span><span> inside_entry </span><span><span class="hljs-keyword">and</span></span><span> current_key:
                entry_text = </span><span><span class="hljs-string">""</span></span><span>.join(current_entry)
                </span><span><span class="hljs-keyword">if</span></span><span> (current_key </span><span><span class="hljs-keyword">not</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> entries) </span><span><span class="hljs-keyword">or</span></span><span> (</span><span><span class="hljs-built_in">len</span></span><span>(entry_text) &gt; </span><span><span class="hljs-built_in">len</span></span><span>(entries[current_key])):
                    entries[current_key] = entry_text
            </span><span><span class="hljs-comment"># Start new entry</span></span><span>
            current_entry = [line]
            inside_entry = </span><span><span class="hljs-literal">True</span></span><span>
