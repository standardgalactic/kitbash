    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(input_file, </span><span><span class="hljs-string">"w"</span></span><span>, encoding=</span><span><span class="hljs-string">"utf-8"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
