6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">sigmoid</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">1</span></span><span> / (</span><span><span class="hljs-number">1</span></span><span> + np.exp(-x))

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">ion_channel_gating</span></span><span>(</span><span><span class="hljs-params">V, L, C, w_V=<span class="hljs-number">1.0</span></span></span><span>, w_L=</span><span><span class="hljs-number">1.0</span></span><span>, w_C=</span><span><span class="hljs-number">1.0</span></span><span>, theta=</span><span><span class="hljs-number">1.5</span></span><span>):
    z = w_V * V + w_L * L + w_C * C - theta
    </span><span><span class="hljs-keyword">return</span></span><span> sigmoid(z)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">run_myelinated_axon_sim</span></span><span>(</span><span><span class="hljs-params">N_nodes=<span class="hljs-number">5</span></span></span><span>, T=</span><span><span class="hljs-number">20</span></span><span>, dt=</span><span><span class="hljs-number">0.1</span></span><span>):
    </span><span><span class="hljs-comment"># Parameters</span></span><span>
    alpha = </span><span><span class="hljs-number">0.8</span></span><span>   </span><span><span class="hljs-comment"># Passive transmission weight</span></span><span>
    beta = </span><span><span class="hljs-number">1.2</span></span><span>    </span><span><span class="hljs-comment"># Ion channel current weight</span></span><span>
    gamma = </span><span><span class="hljs-number">0.5</span></span><span>   </span><span><span class="hljs-comment"># Leak factor</span></span><span>
    
    </span><span><span class="hljs-comment"># Initialize states</span></span><span>
    V = np.zeros((N_nodes, T))        </span><span><span class="hljs-comment"># Voltage at each node over time</span></span><span>
    L = np.zeros(N_nodes)             </span><span><span class="hljs-comment"># Ligand concentration at each node</span></span><span>
    C = np.zeros(N_nodes)             </span><span><span class="hljs-comment"># Ca2+ concentration at each node</span></span><span>
    M = np.ones(N_nodes)              </span><span><span class="hljs-comment"># Myelin integrity between nodes</span></span><span>
    
    </span><span><span class="hljs-comment"># Example stimulus at node 0</span></span><span>
    V[</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>] = </span><span><span class="hljs-number">1.0</span></span><span>
    L[</span><span><span class="hljs-number">2</span></span><span>] = </span><span><span class="hljs-number">0.8</span></span><span>  </span><span><span class="hljs-comment"># Ligand at node 2</span></span><span>
    C[</span><span><span class="hljs-number">3</span></span><span>] = </span><span><span class="hljs-number">0.5</span></span><span>  </span><span><span class="hljs-comment"># Ca2+ at node 3</span></span><span>
    
    </span><span><span class="hljs-keyword">for</span></span><span> t </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(T-</span><span><span class="hljs-number">1</span></span><span>):
        </span><span><span class="hljs-keyword">for</span></span><span> i </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(N_nodes):
            O = ion_channel_gating(V[i,t], L[i], C[i])
            passive_input = alpha * M[i-</span><span><span class="hljs-number">1</span></span><span>] * V[i-</span><span><span class="hljs-number">1</span></span><span>,t] </span><span><span class="hljs-keyword">if</span></span><span> i &gt; </span><span><span class="hljs-number">0</span></span><span> </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span>
            V[i,t+</span><span><span class="hljs-number">1</span></span><span>] = passive_input + beta * O - gamma * V[i,t]
            </span><span><span class="hljs-comment"># Clamp voltage to [0,1] for stability</span></span><span>
            V[i,t+</span><span><span class="hljs-number">1</span></span><span>] = np.clip(V[i,t+</span><span><span class="hljs-number">1</span></span><span>], </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>)
    
    </span><span><span class="hljs-keyword">return</span></span><span> V

</span><span><span class="hljs-comment"># Run simulation</span></span><span>
voltages = run_myelinated_axon_sim()
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"Voltages over time at each node:\n"</span></span><span>, voltages)
</span></span></code></div></div></pre>
