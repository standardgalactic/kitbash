6.01057 5.53708 6.00551 6.00391 6.00391C6.00551 5.53708 6.01057 5.14051 6.03809 4.80371C6.0755 4.34588 6.15483 3.94012 6.34668 3.56348L6.46875 3.34473C6.77282 2.84912 7.20856 2.44514 7.72949 2.17969L7.87207 2.11328C8.20855 1.96886 8.56979 1.90385 8.9707 1.87109C9.42091 1.83432 9.978 1.83496 10.667 1.83496H13.5C14.1891 1.83496 14.746 1.83431 15.1963 1.87109C15.6541 1.90851 16.0599 1.98788 16.4365 2.17969L16.6553 2.30176C17.151 2.60585 17.5548 3.04244 17.8203 3.56348L17.8867 3.70606C18.031 4.04235 18.0962 4.40306 18.1289 4.80371C18.1657 5.25395 18.165 5.81091 18.165 6.5V9.33301Z"></path></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-python"><span><span><span class="hljs-keyword">import</span></span><span> numpy </span><span><span class="hljs-keyword">as</span></span><span> np

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">sigmoid</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">1</span></span><span> / (</span><span><span class="hljs-number">1</span></span><span> + np.exp(-x))

</span><span><span class="hljs-comment"># Ion channel gating MLP (sigmoid activation)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">ion_channel_open_prob</span></span><span>(</span><span><span class="hljs-params">V, L, C, w_V=<span class="hljs-number">1.0</span></span></span><span>, w_L=</span><span><span class="hljs-number">1.0</span></span><span>, w_C=</span><span><span class="hljs-number">1.0</span></span><span>, theta=</span><span><span class="hljs-number">1.5</span></span><span>):
    z = w_V * V + w_L * L + w_C * C - theta
    </span><span><span class="hljs-keyword">return</span></span><span> sigmoid(z)

</span><span><span class="hljs-comment"># Neuron model update (Euler integration)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">neuron_step</span></span><span>(</span><span><span class="hljs-params">V, L, C, I_ext, params, dt</span></span><span>):
    Cm, gL, EL, g_ion, E_ion = params
    O = ion_channel_open_prob(V, L, C)
    
    dVdt = (-gL * (V - EL) - g_ion * O * (V - E_ion) + I_ext) / Cm
    V_new = V + dVdt * dt
    </span><span><span class="hljs-keyword">return</span></span><span> V_new, O

</span><span><span class="hljs-comment"># Example parameters and simulation</span></span><span>
Cm = </span><span><span class="hljs-number">1.0</span></span><span>         </span><span><span class="hljs-comment"># uF/cm^2</span></span><span>
gL = </span><span><span class="hljs-number">0.1</span></span><span>         </span><span><span class="hljs-comment"># mS/cm^2</span></span><span>
EL = -</span><span><span class="hljs-number">65.0</span></span><span>       </span><span><span class="hljs-comment"># mV</span></span><span>
g_ion = </span><span><span class="hljs-number">0.5</span></span><span>      </span><span><span class="hljs-comment"># mS/cm^2</span></span><span>
E_ion = </span><span><span class="hljs-number">0.0</span></span><span>      </span><span><span class="hljs-comment"># mV (e.g., Na+ channel)</span></span><span>
dt = </span><span><span class="hljs-number">0.01</span></span><span>        </span><span><span class="hljs-comment"># ms</span></span><span>
V = -</span><span><span class="hljs-number">65.0</span></span><span>        </span><span><span class="hljs-comment"># initial membrane potential mV</span></span><span>

</span><span><span class="hljs-comment"># Inputs (example)</span></span><span>
L = </span><span><span class="hljs-number">0.8</span></span><span>          </span><span><span class="hljs-comment"># ligand concentration normalized</span></span><span>
C = </span><span><span class="hljs-number">0.3</span></span><span>          </span><span><span class="hljs-comment"># intracellular Ca2+ normalized</span></span><span>
I_ext = </span><span><span class="hljs-number">0.5</span></span><span>      </span><span><span class="hljs-comment"># external input current</span></span><span>

params = (Cm, gL, EL, g_ion, E_ion)

</span><span><span class="hljs-comment"># Simulate for one time step</span></span><span>
V, O = neuron_step(V, L, C, I_ext, params, dt)
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"V: <span class="hljs-subst">{V:<span class="hljs-number">.2</span></span></span></span><span>f} mV, Ion channel open probability: </span><span><span class="hljs-subst">{O:<span class="hljs-number">.3</span></span></span><span>f}")
</span></span></code></div></div></pre>
